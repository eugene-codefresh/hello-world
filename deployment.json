{
  "id": "/dcos/helloworld",
  "instances": 1,
  "cpus": 0.1,
  "mem": 64,
  "container": {
    "type" : "DOCKER",
    "docker": {
      "image": "eugenecodefresh/helloworld:1.0",
      "forcePullImage": false,
      "privileged": false,
      "network": "BRIDGE",
      "portMappings": [
        { "hostPort": 80, "containerPort": 8081, "protocol": "tcp", "name": "http"}
      ]
    }
  },
   "acceptedResourceRoles": [
     "slave_public"
   ]
}
